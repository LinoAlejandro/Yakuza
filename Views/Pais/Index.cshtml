@model IEnumerable<Yakuza.Models.Pais>

@{
    ViewBag.Title = "Index";
}



<br /><br /><br /><br /><br /><br /><br />

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<div class="well">
    <table class="table">
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.nombrePais)
            </th>
            <th></th>
        </tr>

    @foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.nombrePais)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id=item.idPais }) |
                @Html.ActionLink("Details", "Details", new { id=item.idPais }) |
                @Html.ActionLink("Delete", "Delete", new { id=item.idPais })
            </td>
        </tr>
    }
    </table>
</div>